.paniclist{
  display: flex;
  padding: 0 20rpx;
  justify-content: space-between;
  position: relative;
}
.panic_img{
  width: 240rpx;
  height: 240rpx;
}
.panic_right{
  width: 450rpx;
}
.title{
  font-size: 28rpx;
  color: #1F2D3D;
  line-height: 35rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  width: 450rpx;
  height: 70rpx;
}
.schedule{
  margin-top: 40rpx;
}
.schedule_c{
  width: 260rpx;
  height: 20rpx;
  background-color: #DEE5EE;
  border-radius: 10rpx;
  overflow: hidden;
}
.schedule_c text{
  display: block;
  width: 260rpx;
  height: 20rpx;
  background: url("https://youxuan.ecbao.cn/goods/1534919353908_61.png") no-repeat;
  background-size: 260rpx 20rpx;
}
.schedule_n{
  display: block;
  color: #0C1722;
  font-size: 22rpx;
  line-height: 30rpx;
  margin-top: 10rpx;
}
.schedule_n text{
  color: #E31436;
}
.paniclist .panic_price{
  height: 50rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top:20rpx;
}
.price{
  color: #E31436;
  font-size: 34rpx;
}
.panic_remmond{
  height: 95rpx;
  width: 95rpx;
  position: absolute;
  top: 80rpx;
  right: 40rpx;
}
.panic_btn{
  width: 138rpx;
  color: #fff;
  padding: 0;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 26rpx;
  margin: 0;
  border-radius: 25rpx;
}
.panic_btn1{
  background-color: #E31436;
  box-shadow: 0 0 10rpx 0rpx #E31436;
}
.panic_btn2{
  background-color: #CCD4DF;
  box-shadow: 0 0 10rpx 0rpx #CCD4DF;
}
.panic_btn3{
  background-color: #67B936;
  box-shadow: 0 0 10rpx 0rpx #67B936;
}


.classifylist{
  display: flex;
  padding: 0;
  /* justify-content: space-between; */
  flex-direction: column; 
  position: relative;
}
.classify_img{
  width: 224rpx;
  height: 224rpx;

}
.classify_right{
  width: 224rpx;
  position: relative;
}
.classify_title{
  font-size: 28rpx;
  color: #1F2D3D;
  line-height: 35rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  display:-webkit-box; 
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2; 
  width: 220rpx;
  height: 70rpx;
  margin-top: 20rpx;
}
.classify_price{
  display: flex;
  margin-top: 30rpx;
  align-items: flex-end;
}
.outer_price{
  color: #95A9C1;
  font-size: 24rpx;
  line-height: 34rpx;
  text-decoration: line-through;
  margin-left: 10rpx;
}
.inner_price text{
  font-size: 24rpx;
}
.inner_price{
  color: #E31436;
  font-size: 32rpx;
  line-height: 34rpx;
  vertical-align: middle;
  display: flex;
  align-items: center;
}
.inner_price_select{
  color:#1A2D3E;
  font-size:42rpx;
  line-height:34rpx;
  vertical-align:middle;
  display:inline-block;
  align-items:center;
}
.classify_btn{
  width: 66rpx;
  height: 66rpx;
  position: absolute;
  right: 10rpx;
  bottom:20rpx;
}
.indexlist{
  width: 346rpx;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  position: relative;
}
.indexlist_img{
  width: 346rpx;
  height: 346rpx;

}
.indexlist_title{
  width: 320rpx;
  margin: 30rpx auto 0;
  font-size: 28rpx;
  color: #1F2D3D;
  line-height: 35rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  white-space: nowrap;
  height: 35rpx;
}
.indexlist_price{
  padding-left: 10rpx;
  margin: 20rpx 0;
  line-height: 40rpx;
}
.indexlist_price>text{
  color: #E31436;
  font-size: 34rpx;
}
.indexlist_price>text:nth-child(1) text{
  font-size: 24rpx;
}
.indexlist_price>text:nth-child(2){
  color: #95A9C1;
  font-size: 24rpx;
  text-decoration: line-through;
  margin-left:15rpx;
}
.indexlist_recommend_btn{
  width: 180rpx;
  height: 46rpx;
  margin: 0 auto;
  line-height: 46rpx;
  border-radius: 23rpx;
  border: 1rpx solid rgba(153,169,191,1);
  color: #475669;
  font-size: 24rpx;
  font-weight: 400;
  text-align: center;
}
.indexlist_recommends_btn{
  width: 180rpx;
  height: 46rpx;
  margin: 0 auto;
  line-height: 46rpx;
  border-radius: 23rpx;
  border: 1rpx solid rgba(153,169,191,1);
  color: #95A9C1;
  font-size: 24rpx;
  font-weight: 400;
  text-align: center;
}
.classifylist_btns{
  padding-top: 20rpx;
}
.indexlist_share{
  position: absolute;
  right: 0rpx;
  bottom: 14rpx;
  width:100rpx;
  height:46rpx;
  border:1rpx solid rgba(153,169,191,1);
  border-radius:99rpx;
  text-align: center;
  line-height: 46rpx;
  color: #475669;
  font-size: 24rpx;
}
.sell_out{
  position: absolute;
  width: 128rpx;
  height: 128rpx;
  left: 109rpx;
  top: 109rpx;
  background-color: rgba(0,0,0,0.7);
  border-radius: 50%;
}
.classifylist .sell_out{
  left: 48rpx;
  top: 48rpx;
}

/* 选货中心列表 */
.good{
  padding: 28rpx;
  padding-left: 0;
  padding-bottom: 30rpx;
  background-color: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 300rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #E0E6ED;
}
.good-info {
  display:flex;
  flex-direction:column;
  justify-content:space-between;
  margin-left:21rpx;
  width:450rpx;
  height: 240rpx;
}
.good-info .title_select {
  /* font-weight:600; */
  text-overflow:-o-ellipsis-lastline;
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-line-clamp:2;
  -webkit-box-orient:vertical;
  color:#0B1621;
  font-size:32rpx;
  word-break: break-all;
}
.huiyuanzhuan {
  display:flex;
  margin-left: 32rpx;
}
.huiyuanzhuan text:nth-child(1) {
  display:block;
  width:100rpx;
  height:40rpx;
  background-color:#E31436;
  color:#fff;
  font-size:28rpx;
  text-align:center;
  line-height:40rpx;
  position:relative;
  border-radius:3rpx;
}
.huiyuanzhuan text:nth-child(2) {
  padding:0 20rpx 0 10rpx;
  color:#E31436;
  border:1px solid #E31436;
  border-left:none;
  line-height:36rpx;
  font-size:28rpx;
  border-radius:3rpx;
}
.price {
  display:flex;
  align-items:center;
  justify-content: flex-start;
}
.goods_btn button {
  width:128rpx;
  height:60rpx;
  background:linear-gradient(to bottom right,#F46262 ,#E31436);
  box-shadow:2px 0px 9px rgba(227,20,54,0.38);
  color:#fff;
  line-height:60rpx;
  padding:0;
  margin:0 auto;
  font-size:28rpx;
  border-radius:30rpx;
}
.huiyuanzhuan text:nth-child(1)::before{
  position: absolute;
  content: " ";
  width: 0;
  height: 0;
  border-width: 10rpx 0 10rpx 10rpx;
  border-style: solid;
  border-color: transparent transparent transparent #E31436;
  z-index: 99;
  left: -2rpx;
  top: -4rpx;
  transform: rotate(95deg);
}
.select_bottom{
  display: flex;
  justify-content: flex-end;
  padding-top: 21rpx;
  padding-bottom: 20rpx;
}
.select_bottom .recommend_button{
  width:180rpx;
  height:46rpx;
  border:1rpx solid rgba(153,169,191,1);
  border-radius:23rpx;
  font-size:24rpx;
  font-weight:500;
  color:rgba(71,86,105,1);
  line-height:46rpx;
  text-align: center;
  margin-right: 30rpx;
}
.select_bottom .button{
  width:180rpx;
  height:46rpx;
  border:1px solid rgba(153,169,191,1);
  border-radius:23rpx;
  font-size:24rpx;
  font-weight:500;
  color:rgba(71,86,105,1);
  line-height:46rpx;
  text-align: center;
  margin-right: 30rpx;
}
.select_bottom .button:last-child{
  margin-right: 0;
  border:none;
  color: #fff;
  background:linear-gradient(-57deg,rgba(241,33,67,1),rgba(255,96,99,1));
}
.price_about{
  color:#E31436;
  font-size:44rpx;
  line-height:34rpx;
  vertical-align:middle;
  display:inline-block;
  align-items:center;
}
.price_about view{
  display: inline-block;
}