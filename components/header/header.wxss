.header{
  position: fixed;
  width: 750rpx;
  height: 120rpx;
  background-color: #fff;
  z-index: 9;
}
.header_placeholder{
  height: 120rpx;
}
.back{
  position: absolute;
  left: 35rpx;
  top: 50%;
  width: 40rpx;
  height: 35rpx;
  z-index: 100;
}
.back::before{
  content: " ";
  position: absolute;
  height: 20rpx;
  width: 5rpx;
  background-color: #000;
  transform: rotate(45deg);
  top: 0rpx;
  left: 15rpx;
}
.back::after{
  content: " ";
  position: absolute;
  height: 20rpx;
  width: 5rpx;
  background-color: #000;
  transform: rotate(-45deg);
  top: 13rpx;
  left: 15rpx;
}
.content{
  height: 60rpx;
  font-size: 36rpx;
  text-align: center;
  color: #000;
  width: 750rpx;
  position: absolute;
  bottom: 10rpx;
  left: 0;
  z-index: 99;
}

