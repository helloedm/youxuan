}
.scroll-left-box{
  width: 460rpx;
  padding: 0 20rpx;
  position: relative;
}
.scroll-left-text{
  position: absolute;
  left: 0;
}
 .scroll-left-arrow{
  position: absolute;
  top: 50%;
  right:10rpx;
  transform:rotate(135deg);
  width: 30rpx;
  height: 30rpx;
  margin-top: -15rpx;
} 
.scroll-left-arrow::after{
   content: " ";
   width: 20rpx;
   height: 3rpx;
   background-color: #fff;
   position: absolute;
   left: 50%;
   top:50%;
   /* margin: -1rpx 0 0 -7rpx;  */
 }
 .scroll-left-arrow::before{
   content: " ";
   width: 3rpx;
   height: 20rpx;
   background-color: #fff;
   position: absolute;
   left: 50%;
   top:50%;
   /* margin: -7rpx 0 0 -1rpx;  */
 }
