.roll{
   position: absolute;
  top: 20rpx;
  left: 20rpx; 
   display: flex; 
   line-height: 60rpx; 
  width: 560rpx;
  height: 60rpx;
  background:rgba(0,0,0,0.5);
  color:#fff;
  border-radius:30rpx;
  font-size:26rpx;
