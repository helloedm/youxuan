}
.huiyuanzhuan text:nth-child(1)::before{
  position: absolute;
  content: " ";
  width: 0;
  height: 0;
  border-width: 10rpx 0 10rpx 10rpx;
  border-style: solid;
  border-color: transparent transparent transparent #E31436;
  left: 0;
  bottom: -8rpx;
}
.huiyuanzhuan text:nth-child(2){
  padding: 0 20rpx 0 10rpx;
  color: #E31436;
  border: 1rpx solid #E31436;
  border-left: none;
  line-height: 38rpx;
  font-size: 28rpx; 
  border-radius:3rpx;
}
@keyframes  anmiate3{
  from{
    transform: scaleX(1);
  }
  to{
    transform: scaleX(0.9);
  }
}