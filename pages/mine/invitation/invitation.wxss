  width: 125rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: block;
}
.left_item1{
  top: 0;
  left: 0;
  animation: anmiate1 5s;
}
.left_item2{
  top: 0;
  left: 0;
  animation: anmiate2 5s;
}
@keyframes anmiate1{
  0%{left: 0rpx;top:0rpx}
  25%{left:0;top:0rpx}
  50%{left:0;top:0rpx}
  75%{left:0;top:0rpx}
  100%{top: -78rpx;left:0}
}
@keyframes anmiate2{
  0%{left: -208rpx;top:78rpx}
  25%{left:0;top:78rpx}
  50%{left:0;top:78rpx}
  75%{left:0;top:78rpx}
  100%{top: 0;left:0}
}
